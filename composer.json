{
    "require": {
        "php": ">=8.4",
        "nette/di": "^3.2",
        "latte/latte": "^3.0",
        "tracy/tracy": "^2.10"
    },
    "autoload": {
        "classmap": [
          "app/"
        ]
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "squizlabs/php_codesniffer": "^3.13",
        "orisai/coding-standard": "^3.11",
        "phpunit/phpunit": "^12.3",
        "phpstan/phpstan-nette": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
